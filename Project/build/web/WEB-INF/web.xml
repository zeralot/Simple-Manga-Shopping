<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>CusController</servlet-name>
        <servlet-class>controller.CusController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CatController</servlet-name>
        <servlet-class>controller.CatController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProController</servlet-name>
        <servlet-class>controller.ProController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginAdminController</servlet-name>
        <servlet-class>controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>controller.LogoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProControllerClient</servlet-name>
        <servlet-class>controller.ProControllerClient</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CusController</servlet-name>
        <url-pattern>/CusController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CatController</servlet-name>
        <url-pattern>/CatController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProController</servlet-name>
        <url-pattern>/ProController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginAdminController</servlet-name>
        <url-pattern>/LoginAdminController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/LogoutController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProControllerClient</servlet-name>
        <url-pattern>/ProControllerClient</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
